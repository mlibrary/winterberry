            <xs:element ref="section_title"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="type_identifier_list">
        <xs:sequence maxOccurs="unbounded">
            <xs:element ref="identifier"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="type_yesno_nothosted">
        <xs:choice>
            <xs:element ref="yes"/>
            <xs:element ref="no"/>
            <xs:element ref="not_hosted_on_the_platform"/>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="type_yesno_nothosted_notexpire">
        <xs:choice>
            <xs:element ref="yes"/>
            <xs:element ref="no"/>
            <xs:element ref="not_hosted_on_the_platform"/>
            <xs:element ref="permission_does_not_expire"/>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="type_allow_after_expiration">
        <xs:choice>
            <xs:element ref="none"/>
            <xs:element ref="high_res"/>
            <xs:element ref="low_res"/>
            <xs:element ref="not_hosted_on_the_platform"/>
            <xs:element ref="permission_does_not_expire"/>
        </xs:choice>
    </xs:complexType>
    <xs:simpleType name="type_values_contributor_role">
        <xs:restriction base="xs:string">
            <xs:enumeration value="author"/>
            <xs:enumeration value="chair"/>
            <xs:enumeration value="editor"/>
            <xs:enumeration value="reader"/>
            <xs:enumeration value="review-assistant"/>
            <xs:enumeration value="reviewer"/>
            <xs:enumeration value="reviewer-external"/>
            <xs:enumeration value="stats-reviewer"/>
            <xs:enumeration value="translator"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="type_values_isbn_format">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ebook"/>
            <xs:enumeration value="hardcover"/>
            <xs:enumeration value="open access"/>
            <xs:enumeration value="paper"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="type_string" mixed="true">
        <xs:simpleContent>
            <xs:extension base="type_string_nonnull"/>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="type_string_title">
        <xs:simpleContent>
            <xs:restriction base="type_string">
                <xs:maxLength value="200"/>
            </xs:restriction>
        </xs:simpleContent>
    </xs:complexType>
    <xs:simpleType name="type_string_nonnull">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="type_string_doi">
        <xs:simpleContent>
            <xs:restriction base="type_string">
                <xs:pattern value="[0-9.]+/mpub\..*"/>
            </xs:restriction>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="type_string_contributor">
        <xs:complexContent>
            <xs:extension base="type_string">
                <xs:attribute name="role" type="type_values_contributor_role"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="type_string_date">
        <xs:simpleContent>
            <xs:restriction base="type_string">
                <xs:pattern value="(18|19|20)\d\d"/>
                <xs:pattern value="(18|19|20)\d\d[-/.](0[1-9]|1[012])"/>
                <xs:pattern value="(18|19|20)\d\d[-/.](0[1-9]|1[012])[-/.](0[1-9]|[12][0-9]|3[01])"/>
            </xs:restriction>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="type_string_hdl">
        <xs:simpleContent>
            <xs:restriction base="type_string">
                <xs:pattern value="[0-9]+/.*"/>
            </xs:restriction>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="type_string_isbn">
        <xs:simpleContent>
            <xs:restriction base="type_string">
                <xs:pattern value="(978|979)-[0-9]{1,5}-[0-9]{1,7}-[0-9]{1,7}-[0-9]"/>
                <xs:pattern value="(978|979) [0-9]{1,5} [0-9]{1,7} [0-9]{1,7} [0-9]"/>
                <xs:pattern value="[0-9]{1,5}-[0-9]{1,7}-[0-9]{1,7}-[0-9X]"/>
                <xs:pattern value="[0-9]{1,5} [0-9]{1,7} [0-9]{1,7} [0-9X]"/>
            </xs:restriction>
        </xs:simpleContent>
    </xs:complexType>
    <xs:simpleType name="type_string_url">
        <xs:restriction base="xs:anyURI">
            <xs:pattern value='https?://.+'/>
        </xs:restriction>
    </xs:simpleType>

    <xs:group name="type_base_properties">
        <xs:all>
            <xs:element ref="title"/>
        </xs:all>
    </xs:group>

    <xs:complexType name="type_publisherOLD">
        <xs:all>
            <xs:element name="publisher_name" type="type_string"/>
            <xs:element name="publisher_place" type="type_string"/>
        </xs:all>
    </xs:complexType>
    <xs:complexType name="type_personOLD">
        <xs:complexContent>
            <xs:extension base="type_string">
                <xs:attribute name="role" type="type_values_contributor_role"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="type_organizationOLD">
        <xs:complexContent>
            <xs:extension base="type_string"/>
        </xs:complexContent>
    </xs:complexType>

</xs:schema>
